	<queryString language="SQL">
		<![CDATA[SELECT st.first_name As First_Name, 
st.last_name AS Last_Name, st.registration_no AS Registration_No,
sem.semester_no as Semester, sr.semester_gpa AS Semester_GPA, 
sr.cgpa AS CGPA
FROM semester_results as sr
INNER JOIN
    (SELECT student_id, MAX(semester_id) AS semester_id
    FROM semester_results
    GROUP BY student_id) sr_grp 
ON sr.student_id = sr_grp.student_id 
AND sr.semester_id = sr_grp.semester_id
INNER JOIN student as st ON sr.student_id = st.student_id
INNER JOIN semester as sem ON sr.semester_id = sem.semester_id
order by sr.cgpa DESC]]>
	</queryString>
